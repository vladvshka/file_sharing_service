{
	"env": {
		"commonjs": true,
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"import/prefer-default-export": 0,
		"no-console": 0,
		"no-param-reassign": 0,
		"no-await-in-loop": 0,
		"no-extra-boolean-cast": 0,
		"prefer-template": 0,
		"no-bitwise": 0
	}
}
